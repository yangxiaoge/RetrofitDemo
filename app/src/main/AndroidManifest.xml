<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yjn.retrofitdemo">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:name=".advance_retrofit.App"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".advance_retrofit.AdvanceRFActivity"/>
        <activity android:name=".ui.ScrollTitleList"/>
        <activity android:name=".litepal.LitePal_Activity"/>
        <activity android:name=".dialog_edittext.PasswordDialogEditActivity"/>
        <activity android:name=".dialog_customer_layout.CustomerDialogActivity"/>
        <activity android:name=".gank_io_retrofit.activity.GankRetrofitMainActivity"/>
        <activity android:name=".vectorAnimate.VectorAnimateActivity"/>
    </application>

</manifest>